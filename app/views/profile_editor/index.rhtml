<h2><%= _('My profile') %></h2>

<%= display_profile_info(profile) %>

<p> <%= link_to _('Edit'), :action => 'edit' %> </p>

<p> <%= link_to _('Manage members'), :controller => 'profile_members' %> </p>

<p> <%= link_to_cms _('Menage content'), profile.identifier %> </p>

