<% if @search %>
  <ul>
    <% for item in @search %>
      <li>
      <%= item.name %>
      <% form_tag :action => 'add', :id => @region do %>
        <%= hidden_field_tag :validator_id, item.id %>
        <%= submit_tag _('Adicionar') %>
      <% end %>
      </li>
    <% end %>
  </ul>
<% end %>
