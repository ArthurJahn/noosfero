<h1><%= _('Destroy %s') % @community.display_name %></h1>

<p><strong><%= _('Are you sure you want to destroy %s?') % @community.display_name %></strong></p>

<% form_tag do %>
  <%= hidden_field_tag(:confirmation, 1) %>
  <%= submit_button(:ok, _("Yes, I want to destroy.") % @community.display_name) %>
  <%= button(:cancel, _("No, I don't want."), :action => 'index') %>
<% end %>
