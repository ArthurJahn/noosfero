<%= error_messages_for :community %>


<% labelled_form_for :community, @community do |f| %>

  <%= f.text_field :name %>

  <%= f.text_area :description %>

  <% button_bar do %>
    <%= submit_button(:save, _('Create')) %>
    <%= button(:cancel, _('Cancel'), :action => 'index') %>
  <% end %>

<% end %>
